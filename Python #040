def add(a, b):
    return a + b

def subtract(a, b):
    return a - b

def multiply(a, b):
    return a * b

def divide(a, b):
    if b != 0:
        return a / b
    else:
        return "Error: Division by zero"

# Example usage
print(add(5, 3))
print(subtract(10, 7))
print(multiply(2, 4))
print(divide(10, 2))
